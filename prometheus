volumes:
  prometheus-data:
    driver: local

services:
    prometheus:
        container_name: prometheus
        ports:
            - '9091:9090' #modify 9091 to your setup needs
        volumes:
            - '/home/admin/Doker/Prometheus/prometheus.yml:/etc/prometheus/prometheus.yml' #modify the path for your install location
        image: prom/prometheus
        restart: unless-stopped
